<div class="container">
    <div class="jumbotron">
      
      <h2>Please Fill out the Certificate Report</h2>
    
<form action = "/engchecklist/certificatereport" method= "POST">
<input type="hidden" name= "authenticity_token" value="<%=form_authenticity_token %>">

   <div class="form-group">
    <label for="email">Email address</label>
    <input type="email" class="form-control" id="email" name = "engineer[email]">
  </div>
  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" class="form-control" id="password" name = "engineer[password]">
  </div>
   <div class="form-group">
    <label for="password">Confirm Password</label>
    <input type="password" class="form-control" id="password_confirmation" name = "engineer[password_confirmation]">
  </div>
  <input type="submit" name= "submit" class="btn btn-primary submit" placeholder = "Issue the Certificate Report">

</form>
</div>
</div>